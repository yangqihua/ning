.image_box image{width:100%;height:356rpx}.list:nth-child(odd){margin-right:10rpx}.list{background:#fff;width:356rpx;margin-top:10rpx;border:1px solid #efe8ee}.goods{padding:6rpx 12rpx 15rpx 12rpx}.goods .title{display:flex;height:28rpx}.goods .title .logo{width:28rpx;height:28rpx;margin-right:4rpx}.goods .title .product_name{flex:1;color:#333;font-size:28rpx;line-height:28rpx;overflow:hidden;display:-webkit-box;-webkit-line-clamp:1;-webkit-box-orient:vertical}.goods .second{display:flex;justify-content:space-between;color:#999;font-size:22rpx;line-height:32rpx;margin:18rpx 0}.goods .second .quanhou .price{padding-right:6rpx;color:#ff0077;font-size:32rpx;float:left}.goods .second .quanhou .s_price{padding-right:6rpx;float:left}.goods .second .sales{display:flex}.goods .second .sales .label_value{margin-left:6rpx}.goods .third{display:flex;justify-content:space-between;font-size:25rpx;line-height:36rpx;text-align:center;margin-bottom:5rpx}.goods .third .quan{width:280rpx;color:#fff;background:linear-gradient(to right, #ff5777, #ff0077);border:1px solid #ff5777;border-radius:6rpx;margin-right:10rpx}.goods .third .rate{width:380rpx;display:flex;margin-left:20rpx;border:1px solid #f38a33;border-radius:6rpx}.goods .third .rate .rate_text{padding:0 8rpx;background-color:#f38a33;color:#fff}.goods .third .rate .rate_value{flex:1;color:#f38a33}.goods .third .not_agent{width:450rpx;margin-left:5rpx;display:flex;justify-content:flex-end;line-height:40rpx}.goods .third .not_agent .label_text{font-size:22rpx}.goods .third .not_agent .label_value{font-size:32rpx;margin-left:2rpx;color:#ff5777}